{
  "name": "sync-click",
  "version": "1.3.0",
  "description": "A program for clicking on multiple desktops at the same time over the internet with one simple shortcut.",
  "main": "src/index.js",
  "scripts": {
    "setup": "cd src && npm i",
    "rebuild": "cd src/node_modules/robotjs &&  node-gyp rebuild --target=1.6.6 --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "start": "electron .",
    "build": "node build"
  },
  "devDependencies": {
    "electron": "^1.6.6",
    "electron-packager": "^8.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jf908/Sync-Click.git"
  },
